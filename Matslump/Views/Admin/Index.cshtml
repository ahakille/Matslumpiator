@model IEnumerable<Matslump.Models.Users>

@{
    ViewBag.Title = "Index";
}

<h2>Användare</h2>

<p>
    @Html.ActionLink("Skapa ny användare", "NewUser")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConfirmPassword)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConfirmPassword)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.User_id }) |
            @Html.ActionLink("Delete", "#", new { /* id=item.PrimaryKey */ })
        </td>
    </tr>
}

</table>
